<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="web3_transaction_view_tree" model="ir.ui.view">
        <field name="name">web3.transaction.tree</field>
        <field name="model">web3.transaction</field>
        <field name="arch" type="xml">
            <tree string="">
                <field name="name" />
                <field name="origin_address_id" />
                <field name="destination_address_id" />
                <field name="state" />
            </tree>
        </field>
    </record>

    <record id="web3_transaction_view_form" model="ir.ui.view">
        <field name="name">web3.transaction.form</field>
        <field name="model">web3.transaction</field>
        <field name="arch" type="xml">
            <form string="">
                <group name="name" string="string">
                    <field name="name" />
                    <field name="origin_address_id" />
                    <field name="destination_address_id" />
                    <field name="state" />
                </group>
            </form>
        </field>
    </record>

</odoo>
